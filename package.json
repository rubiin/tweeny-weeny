{
  "name": "url-minify",
  "version": "1.0.7",
  "description": "Library to shorten url without any api keys",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "minify": "uglifyjs dist/lib.js --compress --mangle -o dist/lib.js",
    "build": "rimraf dist && tsc -p tsconfig.build.json --declaration && npm run minify",
    "test": "jest"
  },
  "keywords": [
    "utils",
    "urls"
  ],
  "author": "Rubin Bhandari <roobin.bhandari@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rubiin/url-minify"
  },
  "bugs": {
    "url": "https://github.com/rubiin/url-minify/issues"
  },
  "dependencies": {
    "node-fetch": "^2.6.1"
  },
  "main": "dist/lib.js",
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/node": "^15.0.2",
    "@types/node-fetch": "^2.5.11",
    "jest": "^27.0.6",
    "prettier": "^2.3.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.3",
    "typescript": "^4.2.4",
    "uglify-js": "^3.13.10"
  },
  "publishConfig": {
    "access": "public"
  }
}
