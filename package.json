{
  "name": "url-minify",
  "version": "2.2.0",
  "description": "Library to shorten url without any api keys",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "minify": "uglifyjs dist/lib.js --compress --mangle -o dist/lib.js",
    "build": "rimraf dist && tsc -p tsconfig.build.json --declaration && npm run minify",
    "test": "jest"
  },
  "keywords": [
    "utils",
    "urlshortner",
    "urls",
    "shortner"
  ],
  "author": "Rubin Bhandari <roobin.bhandari@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rubiin/url-minify"
  },
  "bugs": {
    "url": "https://github.com/rubiin/url-minify/issues"
  },
  "dependencies": {
    "axios": "^0.21.1"
  },
  "main": "dist/lib.js",
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.12",
    "@types/node-fetch": "^2.5.12",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.4",
    "typescript": "^4.3.5",
    "uglify-js": "^3.14.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
